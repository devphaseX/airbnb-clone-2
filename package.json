{
  "name": "twitter-clone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:gen": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --name update --schema=./src/prisma/schema.prisma",
    "db:pull": "npx prisma db pull --schema=./src/prisma/schema.prisma",
    "db:studio": "npx prisma studio --schema=./src/prisma/schema.prisma",
    "db:dev:rm": "docker compose rm dev-db -s -f -v",
    "db:dev:up": "docker compose up dev-db -d",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.12.0",
    "@types/node": "18.15.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "bcrypt": "^5.1.0",
    "leaflet": "^1.9.3",
    "next": "13.2.4",
    "next-auth": "^4.20.1",
    "next-cloudinary": "^4.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.8.0",
    "react-leaflet": "^4.2.1",
    "react-select": "^5.7.2",
    "typescript": "4.9.5",
    "world-countries": "^4.0.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/leaflet": "^1.9.3",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "prisma": "^4.12.0",
    "tailwindcss": "^3.3.1"
  }
}
